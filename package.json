{
  "name": "react-snippets",
  "description": "vscode snippets for JavaScript and typescript",
  "version": "0.0.1",
  "displayName": "VSCode Snippets",
  "publisher": "brunocarvalhodearaujo <brunocarvalhodearaujo@gmail.com>",
  "license": "MIT",
  "engines": {
    "vscode": "0.10.x"
  },
  "categories": [
    "Extension Packs",
    "Snippets",
    "Keymaps"
  ],
  "extensionDependencies": [
    "PeterJausovec.vscode-docker"
  ],
  "contributes": {
    "snippets": [
      { "language": "javascriptreact", "path": "./src/snippets.json" },
      { "language": "javascript", "path": "./src/snippets.json" },
      { "language": "typescript", "path": "./src/snippets.json" },
      { "language": "typescriptreact", "path": "./src/snippets.json" }
    ],
    "keybindings": [
      {
        "key": "ctrl+b",
        "mac": "cmd+b",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": { "name": "react-component" }
      }
    ]
  }
}
