{
  "name": "react-snippets",
  "description": "vscode snippets for JavaScript and typescript",
  "version": "0.0.1",
  "displayName": "VSCode Snippets",
  "publisher": "brunocarvalhodearaujo",
  "license": "MIT",
  "engines": {
    "vscode": "0.10.x"
  },
  "categories": [
    "Extension Packs",
    "Snippets",
    "Keymaps"
  ],
  "extensionDependencies": [
    "alefragnani.bookmarks",
    "chenxsan.vscode-standardjs",
    "christian-kohler.npm-intellisense",
    "christian-kohler.path-intellisense",
    "codezombiech.gitignore",
    "CoenraadS.bracket-pair-colorizer",
    "eamodio.gitlens",
    "EditorConfig.editorconfig",
    "felixfbecker.php-intellisense",
    "formulahendry.auto-close-tag",
    "lukasz-wronski.ftp-sync",
    "PeterJausovec.vscode-docker",
    "pflannery.vscode-versionlens",
    "PKief.material-icon-theme",
    "wix.vscode-import-cost"
  ],
  "contributes": {
    "snippets": [
      { "language": "javascriptreact", "path": "./src/snippets.json" },
      { "language": "javascript", "path": "./src/snippets.json" },
      { "language": "typescript", "path": "./src/snippets.json" },
      { "language": "typescriptreact", "path": "./src/snippets.json" }
    ],
    "keybindings": [
      {
        "key": "ctrl+b",
        "mac": "cmd+b",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": { "name": "react-component" }
      }
    ]
  }
}
